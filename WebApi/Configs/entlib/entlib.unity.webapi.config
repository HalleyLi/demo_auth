        <register type="SH3H.WAP.Controllers.WapModelController, SH3H.WAP.Controllers"></register>
        <register type="SH3H.WAP.Controllers.WapProducerController, SH3H.WAP.Controllers"></register>
        <register type="SH3H.WAP.Controllers.WapWordController, SH3H.WAP.Controllers"></register>
        <register type="SH3H.WAP.Controllers.WapTagController, SH3H.WAP.Controllers"></register>
        <register type="SH3H.WAP.Controllers.WapMeterRangeController, SH3H.WAP.Controllers"></register>
        <register type="SH3H.WAP.Controllers.WapUserSettingController, SH3H.WAP.Controllers"></register>
        <register type="SH3H.WAP.Controllers.WapUserSettingSchemeController, SH3H.WAP.Controllers"></register>
        
        <!--WebApi.Bulletin-->
        <register type="SH3H.WAP.Bulletin.Controllers.WapBulletinController, SH3H.WAP.Bulletin.Controllers"></register>
        <register type="SH3H.WAP.Bulletin.Controllers.WapBulletinChannelController, SH3H.WAP.Bulletin.Controllers"></register>

      </container>
    </containers>
  </unity>
</configuration>